		kohei.name="上関孝平";
		kohei.age=23;
		System.out.println(kohei.name);
		System.out.println(kohei.age);
		
		Robot aibo=new Robot();
		aibo.name="aibo";
		System.out.println(aibo.name);
		aibo.talk();
		aibo.walk();
		aibo.run();
		
